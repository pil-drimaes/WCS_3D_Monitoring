          "thresholds": { "mode": "absolute", "steps": [{"color": "green"}] },
          "custom": {}
        },
        "overrides": []
      },
      "transformations": [
        {
          "id": "calculateField",
          "options": {
            "alias": "last_exec_time",
            "mode": "reduceRow",
            "reduce": { "reducer": "lastNotNull" },
            "replaceFields": true,
            "binary": {
              "left": "Value",
              "operator": "/",
              "right": 1000

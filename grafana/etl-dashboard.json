      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "etl_last_execution_epoch_ms{domain=\"antRobot\"}",
          "legendFormat": "last_exec_ms",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] },
